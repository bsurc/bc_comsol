<%-
  groups = OodSupport::User.new.groups.sort_by(&:id).tap { |groups|
    groups.unshift(groups.delete(OodSupport::Process.group))
  }.map(&:name).grep(/^P./)
-%>
---
cluster:
  - "borah"
form:
  - account
  - bc_num_hours
  - bc_num_slots
  - num_cores
  - bc_vnc_resolution
  - bc_email_on_started
  - bc_queue
attributes:
  num_cores:
    widget: "number_field"
    label: "Number of cores"
    value: 1
    min: 1
    max: 48
    step: 1
  bc_vnc_resolution:
    required: true
  account: ""
  bc_num_slots: 1
  bc_queue: testondemand
